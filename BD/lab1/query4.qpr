SELECT Препараты.название;
 FROM ;
     Препараты;
 WHERE  ( код_препарата ) IN (SELECT код_препарата FROM (SELECT Список_покупок.код_препарата, SUM(Список_покупок.количество) AS всего, COUNT(количество) AS среднее_число FROM Список_покупок GROUP BY Список_покупок.код_препарата) as b WHERE всего > (SELECT SUM(количество)/COUNT(количество) as re FROM Список_покупок))
